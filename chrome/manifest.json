{
  "manifest_version": 2,
  "name": "Webdext",
  "description": "Intelligent Web Data Extractor",
  "version": "0.0.4",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Webdext",
    "default_popup": "popup.html"
  }
}
